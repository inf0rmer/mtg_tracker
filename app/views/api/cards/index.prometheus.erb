# HELP card_price_eur The price of cards in EUR
# TYPE card_price_eur gauge

<% @cards.each do |card| %>
<% if card.price_eur %>
card_price_eur{oracle_id="<%= card.oracle_id %>",set="<%= card.set %>",collector_number="<%= card.collector_number %>"} <%= card.price_eur %>
<% end %>
<% if card.price_eur_foil %>
card_price_eur{oracle_id="<%= card.oracle_id %>",set="<%= card.set %>",collector_number="<%= card.collector_number %>",foil="true"} <%= card.price_eur_foil %>
<% end %>
<% end %>

